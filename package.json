{
  "name": "phaser-parcel",
  "version": "0.0.1",
  "description": "A Phaser 3 game template using Parcel bundler",
  "main": "src/index.html",
  "scripts": {
    "dist": "parcel build src/index.html -d docs --public-url ./",
    "start": "parcel serve src/index.html",
    "clean": "rm -rfv dist && rm -rf .cache",
    "test": "jest --no-cache --watchAll"
  },
  "keywords": [
    "phaser",
    "phaser3",
    "phaser-boilerplate"
  ],
  "author": "Juan Uys",
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.20",
    "parcel-bundler": "^1.12.4",
    "phaser": "^3.24.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "babel-jest": "^26.6.0",
    "cssnano": "^4.1.10",
    "eslint": "^5.16.0",
    "eslint-config-semistandard": "^13.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^26.6.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "jest": {
    "automock": false
  }
}
